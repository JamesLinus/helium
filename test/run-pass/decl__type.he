fn main
{
    def Point = { x: int, y: int }
    let a: Point;
    a.x = 10;
    a.y = 11;

    assert! (a.x == 10);
    assert! (a.y == 11);

    def IP = [int;4]
    let b: IP;
    b[0] = 127;
    b[1] = 0;
    b[2] = 0;
    b[3] = 1;

    assert! (b[0] == 127);
    assert! (b[1] == 0);
    assert! (b[2] == 0);
    assert! (b[3] == 1);
}
