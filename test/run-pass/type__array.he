fn main
{
    let a: [int;3];
    a[0] = 10;
    a[1] = 11;
    a[2] = 12;

    assert! (a[0] == 10);
    assert! (a[1] == 11);
    assert! (a[2] == 12);


    let b: [{a:int, b:int};2];
    b[0].a = 10;
    b[0].b = 11;
    b[1].a = 12;
    b[1].b = 13;

    assert! (b[0].a == 10);
    assert! (b[0].b == 11);
    assert! (b[1].a == 12);
    assert! (b[1].b == 13);


    let c: [[int;2];2];
    c[0][0] = 10;
    c[0][1] = 11;
    c[1][0] = 13;
    c[1][1] = 14;

    assert! (c[0][0] == 10);
    assert! (c[0][1] == 11);
    assert! (c[1][0] == 13);
    assert! (c[1][1] == 14);
}
